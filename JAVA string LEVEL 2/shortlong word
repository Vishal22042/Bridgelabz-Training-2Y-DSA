class ShortLongWord {

    static int[] find(String[][] arr) {
        int min = 0, max = 0;
        for (int i = 1; i < arr.length; i++) {
            if (Integer.parseInt(arr[i][1]) < Integer.parseInt(arr[min][1])) min = i;
            if (Integer.parseInt(arr[i][1]) > Integer.parseInt(arr[max][1])) max = i;
        }
        return new int[]{min, max};
    }
}
