class VoteCheck {

    static int[] randomAge(int n) {
        int[] a = new int[n];
        for (int i = 0; i < n; i++)
            a[i] = (int)(Math.random() * 90);
        return a;
    }

    static String[][] check(int[] age) {
        String[][] r = new String[age.length][2];
        for (int i = 0; i < age.length; i++) {
            r[i][0] = String.valueOf(age[i]);
            r[i][1] = (age[i] >= 18) ? "true" : "false";
        }
        return r;
    }
}
